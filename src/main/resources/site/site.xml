<?xml version="1.0" encoding="UTF-8"?>
<site>
  <mappings>
    <mapping controller="/site/controllers/search-result.js" order="10">
      <pattern>/search</pattern>
    </mapping>
    <mapping controller="/site/controllers/doc-controller.js" order="10">
      <match>type:'com.enonic.site.developer:doc'</match>
    </mapping>
    <mapping controller="/site/parts/software-show/shield.js" order="10">
      <pattern>.*\/vendors?\/.*shield.svg</pattern>
    </mapping>
    <mapping controller="/lib/enonic/url-redirect/index.js">
      <match>type:'com.enonic.site.developer:url'</match>
    </mapping>
  </mappings>
  <x-data name="menu-item"/>
</site>
